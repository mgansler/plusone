{
  "name": "feeds-dummy-rss-server",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/feeds/dummy-rss-server/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/feeds/dummy-rss-server",
        "main": "apps/feeds/dummy-rss-server/src/main.ts",
        "tsConfig": "apps/feeds/dummy-rss-server/tsconfig.app.json",
        "assets": ["apps/feeds/dummy-rss-server/src/feeds"],
        "isolatedConfig": true,
        "webpackConfig": "apps/feeds/dummy-rss-server/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "feeds-dummy-rss-server:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "feeds-dummy-rss-server:build:development"
        },
        "production": {
          "buildTarget": "feeds-dummy-rss-server:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
