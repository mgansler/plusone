{
  "name": "github-pipeline-status",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/github-pipeline-status/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/github-pipeline-status"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/github-pipeline-status/src/environments/environment.ts",
              "with": "apps/github-pipeline-status/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        }
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "options": {
        "buildTarget": "github-pipeline-status:build",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "github-pipeline-status:build:production",
          "hmr": false
        },
        "development": {
          "buildTarget": "github-pipeline-status:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/github-pipeline-status/**/*.{ts,tsx,js,jsx}"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/github-pipeline-status"],
      "options": {
        "jestConfig": "apps/github-pipeline-status/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "component-test": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/github-pipeline-status/cypress.config.ts",
        "testingType": "component",
        "devServerTarget": "github-pipeline-status:build",
        "skipServe": true
      },
      "configurations": {
        "watch": {
          "watch": true
        }
      }
    }
  },
  "tags": []
}
