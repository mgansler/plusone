        "github-pipeline-status"
      ]
    },
    "dom-utils": {
      "tags": []
    },
    "github-schema": {
      "tags": []
    },
    "hooks": {
      "tags": []
    },
    "quick-zoom": {
      "tags": []
    },
    "quick-zoom-e2e": {
      "tags": [],
      "implicitDependencies": [
        "quick-zoom"
      ]
    }
  }
