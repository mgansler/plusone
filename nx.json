{
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*",
    ".github/workflows/pull_request.yml": "*"
  },
  "affected": {
    "defaultBase": "main"
  },
  "npmScope": "plusone",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "component-test", "e2e"],
        "parallel": 1
      }
    }
  },
  "targetDependencies": {
    "test": [
      {
        "target": "test",
        "projects": "dependencies"
      }
    ],
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "none",
        "linter": "eslint",
        "strict": true,
        "babel": true
      },
      "component": {
        "style": "none"
      },
      "library": {
        "style": "none",
        "linter": "eslint",
        "strict": true
      }
    }
  },
  "defaultProject": "github-pipeline-status",
  "$schema": "./node_modules/nx/schemas/nx-schema.json"
}
