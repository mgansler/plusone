    "dom-utils": {
      "tags": []
    },
    "github-schema": {
      "tags": []
    },
    "hooks": {
      "tags": []
    },
    "quick-zoom": {
      "tags": []
    },
    "quick-zoom-e2e": {
      "tags": [],
      "implicitDependencies": ["quick-zoom"]
    },
    "conference-links": {
      "tags": []
    },
    "github-hooks": {
      "tags": []
    },
    "feeds-backend": {
      "tags": []
    },
    "feeds-frontend": {
      "tags": []
    },
    "feeds-e2e": {
      "tags": [],
      "implicitDependencies": ["feeds-frontend", "feeds-backend"]
    }
  }
