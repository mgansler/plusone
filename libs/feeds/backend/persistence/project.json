{
  "root": "libs/feeds/backend/persistence",
  "sourceRoot": "libs/feeds/backend/persistence/src",
  "projectType": "library",
  "targets": {
    "format": {
      "executor": "./tools/executors:prisma-format"
    },
    "generate": {
      "executor": "./tools/executors:prisma-generate"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/feeds/backend/persistence/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/feeds/backend/persistence"],
      "options": {
        "jestConfig": "libs/feeds/backend/persistence/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
