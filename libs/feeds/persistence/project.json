{
  "name": "feeds-persistence",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/feeds/persistence/src",
  "projectType": "library",
  "tags": [],
  "implicitDependencies": ["plugins-nx-prisma"],
  "targets": {
    "prisma-validate": {
      "executor": "@mgansler/nx-prisma:validate"
    },
    "prisma-format": {
      "executor": "@mgansler/nx-prisma:format"
    },
    "prisma-generate": {
      "executor": "@mgansler/nx-prisma:generate"
    },
    "prisma-deploy": {
      "executor": "@mgansler/nx-prisma:deploy",
      "dependsOn": [
        {
          "target": "docker-compose"
        }
      ]
    },
    "prisma-migrate": {
      "executor": "@mgansler/nx-prisma:migrate"
    },
    "prisma-push": {
      "executor": "@mgansler/nx-prisma:push",
      "dependsOn": [
        {
          "target": "prisma-generate"
        }
      ]
    },
    "docker-compose": {
      "executor": "@mgansler/nx-prisma:docker-compose",
      "options": {
        "path": "../../../apps/feeds/docker-compose.database.yml",
        "readyLogStatement": "database system is ready to accept connections"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
