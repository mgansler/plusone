{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/dinner-plan/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/dinner-plan/**/*.{ts,tsx,js,jsx}"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/dinner-plan"],
      "options": {
        "jestConfig": "libs/dinner-plan/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "component-test": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "libs/dinner-plan/cypress.config.ts",
        "testingType": "component"
      },
      "configurations": {
        "watch": {
          "watch": true
        }
      }
    }
  },
  "tags": []
}
