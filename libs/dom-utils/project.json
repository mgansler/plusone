{
  "root": "libs/dom-utils",
  "sourceRoot": "libs/dom-utils/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/dom-utils/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/dom-utils"],
      "options": {
        "jestConfig": "libs/dom-utils/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
