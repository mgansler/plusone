{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "github-pipeline-status",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "none",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "none"
      },
      "library": {
        "style": "none",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "github-pipeline-status": {
      "root": "apps/github-pipeline-status",
      "sourceRoot": "apps/github-pipeline-status/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/github-pipeline-status",
            "index": "apps/github-pipeline-status/src/index.html",
            "main": "apps/github-pipeline-status/src/main.tsx",
            "polyfills": "apps/github-pipeline-status/src/polyfills.ts",
            "tsConfig": "apps/github-pipeline-status/tsconfig.app.json",
            "assets": [
              "apps/github-pipeline-status/src/favicon.ico",
              "apps/github-pipeline-status/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/github-pipeline-status/src/environments/environment.ts",
                  "with": "apps/github-pipeline-status/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "github-pipeline-status:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "github-pipeline-status:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/github-pipeline-status/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/github-pipeline-status"
          ],
          "options": {
            "jestConfig": "apps/github-pipeline-status/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "github-pipeline-status-e2e": {
      "root": "apps/github-pipeline-status-e2e",
      "sourceRoot": "apps/github-pipeline-status-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/github-pipeline-status-e2e/cypress.json",
            "tsConfig": "apps/github-pipeline-status-e2e/tsconfig.e2e.json",
            "devServerTarget": "github-pipeline-status:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "github-pipeline-status:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/github-pipeline-status-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "github-schema": {
      "root": "libs/github-schema",
      "sourceRoot": "libs/github-schema/src",
      "projectType": "library",
      "targets": {
        "codegen": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "graphql-codegen --config libs/github-schema/codegen.yml"
              }
            ]
          }
        }
      }
    },
    "hooks": {
      "root": "libs/hooks",
      "sourceRoot": "libs/hooks/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/hooks/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/hooks"
          ],
          "options": {
            "jestConfig": "libs/hooks/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "input": {
      "root": "libs/input",
      "sourceRoot": "libs/input/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/input/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/input"
          ],
          "options": {
            "jestConfig": "libs/input/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
